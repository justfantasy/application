{
    "name": "justfantasy/yii2-react-uniapp-base-app",
    "description": "Yii 2 react uniapp base Template",
    "keywords": ["yii2", "react", "uniapp", "base app"],
    "homepage": "https://github.com/justfantasy/application",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/justfantasy/application/issues?state=open",
        "source": "https://github.com/justfantasy/application"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap4": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0 || ~2.1.0",
        "yiisoft/yii2-queue": "^2.3",
        "yiisoft/yii2-redis": "^2.0",
        "overtrue/wechat": "~4.0",
        "sizeg/yii2-jwt": "^2.0",
        "guzzlehttp/guzzle": "^7.4",
        "ext-json": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.2.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/module-asserts": "^1.0",
        "codeception/module-yii2": "^1.0",
        "codeception/module-filesystem": "^1.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "symfony/browser-kit": ">=2.7 <=4.2.4",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "easywechat-composer/easywechat-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
